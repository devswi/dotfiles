{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "useGitignore": true,
  "allowCompoundWords": true,
  "globRoot": ".",
  "ignorePaths": [
    ".vscode",
    "node_modules",
    "dist",
    "build"
  ],
  "words": [
    "bson",
    "codeium",
    "cspell",
    "devswi",
    "migrateup",
    "neovim",
    "nvim",
    "rgba",
    "shortid",
    "uber",
    "yizhi"
  ],
  "enabledFileTypes": {
    "go.sum": false,
    "go.mod": false
  }
}